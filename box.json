{
  "alias": "slh.phar",
  "directories": [
    "src",
    "vendor"
  ],
  "compression": "GZ",
  "stub": true,
  "finder": [
    {
      "notName": "/.*\\.md|.*\\.dist|Makefile|composer\\.json|composer\\.lock/",
      "exclude": ["Tests", "tests", "doc", "docs", "Fixtures", "bin", "crazywhalecc"],
      "in": "vendor"
    },
    {
      "notName": "/.*\\.md|.*\\.dist|Makefile|composer\\.json|composer\\.lock/",
      "exclude": ["source", "dist", "images"],
      "in": "./"
    }
  ],
  "git-version": "package_version",
  "output": "dist/slh.phar",
  "check-requirements": false
}